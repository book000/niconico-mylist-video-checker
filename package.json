{
  "name": "niconico-mylist-video-checker",
  "version": "2.0.0",
  "main": "dist/main.js",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "start": "node ./dist/main.js",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m",
    "clean": "rimraf dist output",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/config": "3.3.1",
    "@types/node": "18.18.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vercel/ncc": "0.38.0",
    "axios": "1.5.1",
    "config": "3.3.9",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-promise": "6.1.1",
    "logform": "2.5.1",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1",
    "yarn-run-all": "3.1.1"
  }
}
